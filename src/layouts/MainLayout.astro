---
import Navbar from "../components/Navbar.astro"
import MainHead from "./MainHead.astro"

const { title, seoTitle, seoDesc } = Astro.props
---

<html>
    <MainHead title={ title } seoTitle={ seoTitle } seoDesc={ seoDesc }/>
    <body class="bg-[#29303B] text-slate-300">
        <header>
            <Navbar/>
        </header>
        <div class="block h-10"></div>
        <div class="px-0 md:px-21 pt-0 overflow-y-auto custom-scrollbar overscroll-none" style="height: 100dvh;"> 
            <slot/>
        </div>
    </body>
</html>